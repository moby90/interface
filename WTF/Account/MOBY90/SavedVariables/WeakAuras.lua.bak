					["custom"] = "aura_env.souls=0\naura_env.corrosionList={}\n\naura_env.addSoul=function()\n    aura_env.souls=aura_env.souls+1\nend\n\naura_env.removeSoul=function(unit)\n    if aura_env.souls > 0 then \n        local now=GetTime()\n        -- For a specific unit, don't remove souls more frequent than 2 seconds\n        if now - (aura_env.corrosionList[unit] or 0) > 2 then\n            aura_env.souls=aura_env.souls-1 \n            aura_env.corrosionList[unit]=now\n        end\n    end\nend\n\naura_env.clearSouls=function()\n    aura_env.souls=0\nend\n\naura_env.harvest=0\naura_env.nextHarvest=nil\naura_env.harvestTimer={\n    [14]={48,81,98}, --normal\n    [15]={42,70.5,86}, -- heroic\n    [16]={}, -- mythic (need more info)\n    [17]={}} -- LFR (need more info)\n\naura_env.formatNumber=function(number)\n    if number>=1000000 then number=string.format(\"%.2fm\", number/1000000)\n    else number=string.format(\"%dk\", number/1000)\n    end\n    return number\nend\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
