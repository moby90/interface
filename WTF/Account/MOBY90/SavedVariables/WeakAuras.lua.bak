			["actions"] = {
				["start"] = {
				},
				["init"] = {
					["custom"] = "--Made by Pattisaurus@ScrubBusters\n--The position of the icon on the raid frames\n--Available position options: \"CENTER\", \"BOTTOM\", \"TOP\", \"LEFT\", \"RIGHT\", \"BOTTOMLEFT\", \"BOTTOMRIGHT\", \"TOPLEFT\", \"TOPRIGHT\"\naura_env.position = \"CENTER\"\n--The height and width of the icon to show on the raid frames\naura_env.iconHeight = 30\naura_env.iconWidth = 30\n\n--The alpha of the icon, 1 is max 0 is minimum. The lower the number the more faded out it will be.\naura_env.iconAlpha = 0.8\n\n--DO NOT EDIT BELOW--\nRegisterAddonMessagePrefix(\"SB_DAGGERS_FA\")\naura_env.spellId = 236573\naura_env.debuffDuration = 5\n\naura_env.addIcon = function(parentFrame)\n    local frame = CreateFrame(\"Frame\",nil,parentFrame)\n    frame:SetFrameStrata(\"HIGH\")\n    frame:SetWidth(aura_env.iconWidth)\n    frame:SetHeight(aura_env.iconHeight)\n    frame:SetAlpha(aura_env.iconAlpha)\n    \n    local texture = frame:CreateTexture(nil,\"HIGH\")\n    texture:SetTexture(select(3, GetSpellInfo(aura_env.spellId)))\n    texture:SetAllPoints(frame)\n    frame.texture = texture\n    \n    frame:SetPoint(aura_env.position,0,0)\n    \n    local cooldown = CreateFrame(\"COOLDOWN\", nil, frame, \"CooldownFrameTemplate\")\n    cooldown:SetCooldown(GetTime(), aura_env.debuffDuration)\n    cooldown:SetAllPoints(frame)\n    cooldown:SetDrawEdge(false)\n    cooldown:SetHideCountdownNumbers(IsAddOnLoaded(\"OmniCC\") or false)\n    \n    frame:Show()\n    C_Timer.After(aura_env.debuffDuration, function()\n            frame:Hide()\n    end)\nend\n\naura_env.highlight = function(target)\n    local hasGrid2 = IsAddOnLoaded(\"Grid2\")\n    local hasElvUI = _G[\"ElvUF_Raid\"] and _G[\"ElvUF_Raid\"]:IsVisible()\n    \n    if hasElvUI then\n        for i=1, 8 do\n            for j=1, 5 do\n                local f = _G[\"ElvUF_RaidGroup\"..i..\"UnitButton\"..j]\n                if f and f.unit and UnitName(f.unit) == target then\n                    aura_env.addIcon(f)\n                    return\n                end\n            end\n        end\n    elseif hasGrid2 then\n        local layout = Grid2LayoutFrame\n        \n        if layout then\n            local children = {layout:GetChildren()}\n            for _, child in ipairs(children) do\n                if child:IsVisible() then\n                    local frames = {child:GetChildren()}\n                    for _, f in ipairs(frames) do\n                        if f.unit and UnitName(f.unit) == target then\n                            aura_env.addIcon(f)\n                            return\n                        end\n                    end\n                end\n            end\n        end\n    else\n        for i=1, 40 do\n            local f = _G[\"CompactRaidFrame\"..i]\n            if f and f.unitExists and f.unit and UnitName(f.unit) == target then\n                aura_env.addIcon(f)\n                return\n            end\n        end\n        for i=1, 4 do\n            for j=1, 5 do\n                local f = _G[\"CompactRaidGroup\"..i..\"Member\"..j]\n                if f and f.unitExists and f.unit and UnitName(f.unit) == target then\n                    aura_env.addIcon(f)\n                    return\n                end\n            end\n        end\n    end\nend",
