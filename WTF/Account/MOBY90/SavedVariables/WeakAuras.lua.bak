					["custom"] = "aura_env.Last = 0\naura_env.check = 0\n\naura_env.Say = function(Mark2)\n    if GetTime()-1 >= aura_env.Last then\n        aura_env.Last = GetTime()\n        if Mark2 == 1 then\n            Mark3 = 1\n        elseif Mark2 == 2 then\n            Mark3 = 2\n        elseif Mark2 == 3 then\n            Mark3 = 3\n        elseif Mark2 == 4 then\n            Mark3 = 4\n        end\n        SendChatMessage(\"{rt\"..Mark3..\"}\",\"YELL\") \n    end\nend\n\n\n\nfunction round(num, numDecimalPlaces)\n    if numDecimalPlaces and numDecimalPlaces>0 then\n        local mult = 10^numDecimalPlaces\n        return math.floor(num * mult + 0.5) / mult\n    end\n    return math.floor(num + 0.5)\nend\n\nfunction printg(Star, Circle, Dia, Tri)\n    \n    local star = \"\"\n    local circle = \"\"\n    local dia = \"\"\n    local tri = \"\"\n    \n    \n    if #Star > 0 and #Circle > 0 and #Dia > 0 and #Tri > 0 then\n        for i =1,#Star do\n            star = star .. \" \" .. Star[i]\n        end\n        for i = 1,#Circle do\n            circle = circle .. \" \" .. Circle[i]\n        end\n        for i = 1,#Dia do\n            dia = dia .. \" \" .. Dia[i]\n        end\n        for i = 1,#Tri do\n            tri = tri .. \" \" .. Tri[i]\n        end\n        print(\"Star \" .. #Star)\n        print(\"Circle \" .. #Circle)\n        print(\"Dia \" .. #Dia)\n        print(\"Tri \" .. #Tri)\n    end\nend\n\n\n\n\n\n\n",
